{
	"welcomeFile": "/ui/index.html",
	"authenticationMethod": "none",
	"logout": {
		"logoutEndpoint": "/do/logout"
	},
	"routes": [{
		"source": "^/ui/(.*)$",
		"target": "$1",
		"localDir": "webapp"
	}, {
		"source": "^/server/(.*)$",
		"destination": "server",
		"target": "$1",
		"csrfProtection": false,
		"authenticationType": "none"
	}]
}